  "main": "servers/main.js",
  "scripts": {
    "local": "source ./env/local.env; npm-run-all build --parallel server",
    "test": "source ./env/test.env; npm-run-all build --parallel server",
    "stage": "source ./env/stage.env; npm-run-all env-stage build --parallel server",
    "production": "source ./env/production.env; npm-run-all env-stage build --parallel server",
    "build": "webpack --config config/webpack.config.js --mode production",
    "build-watch": "webpack --config ./config/webpack.config.js --watch",
    "server": "nodemon --ignore ./data servers/main.js",
    "server-watch": "nodemon --watch ./dist --ignore ./data servers/main.js"
  },
